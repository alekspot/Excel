{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "semi": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "comma-dangle": "off",
        "no-invalid-this": "off",
        "object-curly-spacing": "off",
        "operator-linebreak": "off",
        "max-len": "off",
        "no-debugger": "off",
        "no-unused-vars": "off",
        "no-console": "error"
    },
    "extends": ["eslint:recommended", "google"]
}